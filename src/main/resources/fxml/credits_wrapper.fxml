<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>

<BorderPane fx:id="creditsPane" maxHeight="300.0" maxWidth="500.0" minHeight="300.0" minWidth="500.0" styleClass="creditsPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dezorganizacja.bridge.view.graphical.CreditsController">
   <stylesheets>
      <URL value="@../css/credits.css" />
   </stylesheets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER_RIGHT">
         <children>
            <fx:include fx:id="creditsBox" source="credits.fxml" />
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <Button alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#closeCredits" text="Close credits" BorderPane.alignment="BOTTOM_RIGHT" />
   </bottom>
</BorderPane>
